{
  "name": "@haemomedtec/node-signalr",
  "version": "3.1.1-rev.6",
  "description": "A signalR client for Node.JS",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Oliver Nybo",
    "email": "oliver@haemomedtec.dk"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HaemoMedtec/node-signalr.git"
  },
  "bugs": {
    "url": "https://github.com/HaemoMedtec/node-signalr/issues"
  },
  "homepage": "https://github.com/HaemoMedtec/node-signalr#readme",
  "keywords": [
    "signalr",
    "websockets"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --ext .ts src/**",
    "typecheck": "tsc --noEmit",
    "build": "npm run lint && unbuild && uglifyjs-folder dist -eo dist -p *cjs -x .cjs && uglifyjs-folder dist -eo dist -p *mjs -x .mjs"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/ws": "^8.5.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "uglifyjs-folder": "^3.3.0",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "tiny-typed-emitter": "^2.1.0",
    "ws": "^8.16.0"
  }
}
